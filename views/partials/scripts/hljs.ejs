<script src="https://cdn.jsdelivr.net/gh/highlightjs/cdn-release@11.8.0/build/highlight.min.js" integrity="sha384-g4mRvs7AO0/Ol5LxcGyz4Doe21pVhGNnC3EQw5shw+z+aXDN86HqUdwXWO+Gz2zI" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/gh/highlightjs/cdn-release@11.8.0/build/languages/json.min.js" integrity="sha384-H1NGK0vlxZxEgoNoQLEc3If3KzPzzjsQtJUqk+PXyJzroAXFb1XOE+cTpJUShk3S" crossorigin="anonymous"></script>
<!-- TODO: If you require highlights for other languages, you can add them here... -->
<script src="https://cdn.jsdelivr.net/npm/highlightjs-line-numbers.js@2.8.0/dist/highlightjs-line-numbers.min.js" integrity="sha384-+ch8x/dgaV//v6Sa8m4v5+7KScnpCuxHqilN8njQ013CEKg3Fbd8Q3oN9tfpouLh" crossorigin="anonymous"></script>
<script>
    document.addEventListener("DOMContentLoaded", () => {
        document.querySelectorAll("pre code").forEach((element) => {
            hljs.highlightElement(element);
            element.innerHTML = element.innerHTML
                .replaceAll(/&lt;START&gt;/gm,"<mark>")
                .replaceAll(/&lt;END&gt;/gm,"</mark>");
            if (!element.classList.contains("language-plaintext")) {
                hljs.lineNumbersBlock(element, { singleLine: true });
            }
        });
    });
</script>